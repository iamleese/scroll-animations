{
	"name": "scroll-animations",
	"version": "0.1.0",
	"description": "Smooth scrolling animation with the help of locomotive.js",
	"author": "Melissa Hiatt",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "concurrently \"webpack --config webpack.config.js\" \"wp-scripts build\" ",
